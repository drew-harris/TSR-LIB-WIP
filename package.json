    "@ssrx/renderer": "^0.4.1",
    "@ssrx/streaming": "^0.2.0",
    "@ssrx/vite": "latest",
    "@tanstack/react-query": "^5.36.0",
    "@tanstack/react-query-devtools": "^5.36.0",
    "@tanstack/react-router": "1.32.0",
    "@tanstack/router-cli": "^1.30.0",
    "@tanstack/router-devtools": "1.32.0",
    "@tanstack/router-vite-plugin": "1.31.18",
    "@tanstack/start": "^1.32.1",
    "@trpc/client": "11.0.0-rc.366",
    "@trpc/react-query": "11.0.0-rc.366",
    "@trpc/server": "11.0.0-rc.366",
